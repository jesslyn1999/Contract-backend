        "axios": "^0.19.2",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "connect-mongo": "^3.2.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-session": "^1.17.0",
        "imagemin": "^7.0.1",
        "imagemin-pngquant": "^8.0.0",
        "json2csv": "^4.5.4",
        "lodash": "^4.17.15",
        "moment": "^2.24.0",
        "mongodb-memory-server": "^6.6.0",
        "mongoose": "^5.8.2",
        "multer": "^1.4.2",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
